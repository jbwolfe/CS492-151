		this.songsOf70s = newSongsOf70s;
		this.songsOf80s = newSongsOf80s;
		this.songsOf90s = newSongsOf90s;
		
		this.iter70sSongs = this.songsOf70s.createIterator();
	}
	
	public void showSongs() {
		
	}
}
